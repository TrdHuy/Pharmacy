<UserControl x:Class="Pharmacy.Implement.Windows.PopupScreenWindow.MVVM.Views.UserControls.PrintPreviewControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:hps="clr-namespace:HPSolutionCCDevPackage.netFramework;assembly=HPSolutionCCDevPackage.netFramework"
             xmlns:local="clr-namespace:Pharmacy.Implement.Windows.PopupScreenWindow.MVVM.Views"
             xmlns:buca="clr-namespace:Pharmacy.Implement.Windows.BaseWindow.MVVM.Views.Animations"
             xmlns:report="clr-namespace:Microsoft.Reporting.WinForms;assembly=Microsoft.ReportViewer.WinForms"
             mc:Ignorable="d"
             d:DesignHeight="600"
             d:DesignWidth="800">
    <WindowsFormsHost Grid.Row="2"
                      Margin="10">
        <report:ReportViewer x:Name="Report"
                             ShowToolBar="True" />
    </WindowsFormsHost>
</UserControl>
